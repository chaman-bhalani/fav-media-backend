    "render-build": "npm install @types/node && npx prisma migrate deploy && npm run build"
  },
  "dependencies": {
    "@prisma/client": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "zod": "^3.24.1",
    "prisma": "^6.18.0",
    "@types/node": "^22.18.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "dotenv-cli": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
